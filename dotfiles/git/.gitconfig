[user]
  name = Mitsuhiro Tanda
[core]
  excludesfile = ~/.gitignore_global
  #autocrlf = input
  editor = vim
  pager = "LC_ALL=ja_JP.UTF-8 lv -c"
[push]
  #default = tracking
[rebase]
  stat = true
[color]
  #ui = auto
  status = auto
  diff = auto
  branch = auto
  interactive = auto
  grep = auto
[alias]
  st = status
  up = pull --rebase
  ci = commit -a
  co = checkout
  diffw = diff --color-words
  diffn = diff --no-ext-diff
[diff]
  external = git-diff-wrapper
[pager]
  diff =
[credential]
  helper = cache --timeout=3600
[include]
  path = .gitconfig.local
